<!-- : Tabs -->
<div class="tabpanel--stretch-mobile" data-module="tabpanel">
  <ul class="tabpanel__list">
  	{{#each this }}
    <li class="tabpanel__tab{{#if active}} is-active{{/if}} {{#if disabled}} is-disabled{{/if}}" ><a href="#">{{label}}</a></li>
    {{/each}}
  </ul>
</div>
