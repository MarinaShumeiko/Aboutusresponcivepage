    {{#if links}}
      {{#each links}}
      <li><a href="#{{anchor}}">{{text}}</a></li>
      {{/each}}
    {{else}}
    <li><a href="#donotwork">My Phone doesn't work</a></li>
    <li><a href="#brokenphone">Broken phone</a></li>
    <li><a href="#lostphone">Lost phone</a></li>
    {{/if}}
